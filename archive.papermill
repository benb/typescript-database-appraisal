length to modulate ventricular pressure development.","institution":"Department of Cell and Molecular Physiology, Stritch School of Medicine, Loyola University Chicago, Chicago, IL 60153, USA. pdetombe@lumc.edu","keywords":[{"canonical_name":"actin cytoskeleton","created_at":1370463781.174269,"uuid":"D02AB97C-DAF4-4D1F-99DC-59BFA9893FFE","is_mine":0,"type":0,"updated_at":1370463781.174768,"name":"Actin Cytoskeleton"},{"canonical_name":"animals","created_at":1338577435.2526879,"uuid":"99B1A15C-DB90-4782-8E35-5FB6FCE1B32B","is_mine":0,"type":0,"updated_at":1338577435.25509,"name":"Animals"},{"canonical_name":"heart","created_at":1346815998.6449289,"uuid":"1EC4F647-94EA-425C-A7A6-F48F0E627BE1","is_mine":0,"type":0,"updated_at":1346815998.6451659,"name":"Heart"},{"canonical_name":"humans","created_at":1346815998.4132409,"uuid":"D3284EF2-2F81-44CF-A8E0-4513BF1359E0","is_mine":0,"type":0,"updated_at":1346815998.4134309,"name":"Humans"},{"canonical_name":"models biological","created_at":1346815998.489609,"uuid":"5B900309-D29F-4E3E-A7BB-754F8ABB6A49","is_mine":0,"type":0,"updated_at":1346815998.4898171,"name":"Models, Biological"},{"canonical_name":"myocardial contraction","created_at":1346815998.563787,"uuid":"160EC444-5C7D-4090-A3AA-7FEC111AC912","is_mine":0,"type":0,"updated_at":1346815998.5640349,"name":"Myocardial Contraction"},{"canonical_name":"sarcomeres","created_at":1346815998.410219,"uuid":"71841D23-7F93-4FB4-A649-C7062AE48CFF","is_mine":0,"type":0,"updated_at":1346815998.4104121,"name":"Sarcomeres"},{"canonical_name":"troponin i","created_at":1370463547.953572,"uuid":"3EA4B095-8DEC-4787-8778-7C424B8A1ACC","is_mine":0,"type":0,"updated_at":1370463547.9545579,"name":"Troponin I"}],"authors":[{"uuid":"482BD4EF-50A0-4BCA-AED4-A69D466E9940","name_string":"[1] Tombe [2] de [4] Pieter [5] P ","prename":"Pieter P","label":0,"firstName":"Pieter","lastName":"Tombe","initial":"T","created_at":1370454067.843013,"surname":"de Tombe","flagged":0,"institutional":0,"privacy_lev